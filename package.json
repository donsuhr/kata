{
  "name": "kata",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all --serial clean copy:images && NODE_ENV=production npm-run-all --parallel build:*",
    "build:html": "eleventy --input=./src",
    "build:scripts": "babel src --out-dir _site --extensions .ts",
    "build:styles": "npm run styles -- --style=compressed",
    "clean": "rm -rf ./_site && rm -rf .tmp",
    "copy:images": "copyfiles --up 1 --verbose src/**/*.{png,jpg,webp,woff,woff2,svg} _site",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit",
    "lint:styles": "stylelint \"./src/**/*.{css,scss,sass}\" --cache ",
    "scripts:watch": "babel src --out-dir _site --extensions .ts --watch",
    "serve": "eleventy --serve --input=./src",
    "start": "npm run clean && npm-run-all --serial clean copy:images && npm run styles && npm-run-all --parallel serve styles:watch scripts:watch",
    "styles": "node-sass src -o .tmp --source-map true && postcss .tmp/**/*.css --base .tmp --dir _site --verbose",
    "styles:watch": "concurrently \"sass src:.tmp --watch\" \"postcss .tmp/**/*.css --verbose --base .tmp --dir _site --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "one": "sass src:.tmp",
    "onea": "node-sass src -o .tmp --source-map true",
    "two": "postcss .tmp/**/*.css --verbose --base .tmp --dir _site "
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-minifier-terser": "^7.1.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-strict-value": "^1.9.1",
    "stylelint-order": "^5.0.0"
  }
}
