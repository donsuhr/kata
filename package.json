{
  "name": "kata",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm-run-all --serial clean copy:images && NODE_ENV=production npm-run-all --parallel build:*",
    "build:html": "eleventy --input=./src",
    "build:scripts": "babel src --out-dir dist/kata --extensions .ts,.tsx",
    "build:styles": "npm run styles -- --style=compressed",
    "build:viteProjects": "node ./scripts/build-vite-projects.mjs",
    "clean": "rm -rf ./dist && rm -rf .tmp",
    "copy:images": "copyfiles --up 1 --verbose src/**/*.{png,jpg,webp,woff,woff2,svg,json} dist/kata",
    "lint": "npm-run-all --serial --continue-on-error lint:*",
    "lint:scripts": "eslint . --ext .js,.jsx,.ts,.tsx,.cjs",
    "lint:scripts:fix": "prettier . --write && npm run lint:scripts -- --fix",
    "lint:styles": "stylelint \"./src/**/*.{css,scss,sass}\" --cache ",
    "lint:types": "tsc  --noEmit",
    "scripts:watch": "babel src --out-dir dist/kata --extensions .ts --watch",
    "serve": "eleventy --serve --input=./src",
    "start": "npm run clean && npm-run-all --serial clean copy:images && npm run styles && npm-run-all --parallel serve styles:watch scripts:watch watchCurrentViteProject",
    "styles": "node-sass src -o .tmp --source-map true && postcss .tmp/**/*.css --base .tmp --dir dist/kata --verbose",
    "styles:watch": "concurrently \"node-sass src -o .tmp --source-map true --watch\" \"postcss .tmp/**/*.css --verbose --base .tmp --dir dist/kata --watch\"",
    "test": "vitest --coverage --coverage.all=false --config ./src/advent_2021/02_e-commerce-component--react/vite.config.mjs --dir ./src/advent_2021/02_e-commerce-component",
    "watchCurrentViteProject": "vite build ./src/advent_2021/02_e-commerce-component--react --watch --outDir=../../../dist/kata/advent_2021/02_e-commerce-component--react --base=./ --emptyOutDir=false --sourcemap=true"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-minifier-terser": "^7.2.0",
    "jsdom": "^25.0.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-declaration-strict-value": "^1.10.6",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
